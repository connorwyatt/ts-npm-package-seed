{
  "defaultSeverity": "error",
  "extends": [
    "tslint:all"
  ],
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "no-import-side-effect": false,
    "no-inferrable-types": false,
    "array-type": [
      true,
      "generic"
    ],
    "prefer-function-over-method": false
  }
}
